def showNum():
    for i in range(1000000) :
        if i%100 == 0 :
            print(str(i) + "\n" , end="")
        else :
            print(i, end="")


def showMunja():
    for i in range(1000000) :
        if i%100 == 0 :
            print(char(i) + "\n" , end="")
        else :
            print(char(i), end="")



if __name__ == "__main__":
    showNum()
    showMunja()